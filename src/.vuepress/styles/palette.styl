$accentColor = #9EAED8
$textColor = #2c3e50
$borderColor = #eaecef
$codeBgColor = #282c34

@font-face {
  font-family: 'Circular Std';
  src: url('/docs/fonts/CircularStd-MediumItalic.eot');
  src: local('Circular Std Medium Italic'), local('CircularStd-MediumItalic'),
      url('/docs/fonts/CircularStd-MediumItalic.eot?#iefix') format('embedded-opentype'),
      url('/docs/fonts/CircularStd-MediumItalic.woff2') format('woff2'),
      url('/docs/fonts/CircularStd-MediumItalic.woff') format('woff'),
      url('/docs/fonts/CircularStd-MediumItalic.ttf') format('truetype');
  font-weight: 500;
  font-style: italic;
}

